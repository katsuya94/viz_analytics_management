<% content_for :head do %>
<script>
$(function() {
	$('#chart').highcharts({
		chart: {
			type: 'line'
		},
		title: {
			text: '<%= @m.name.underscore.humanize %>'
		},
		xAxis: {
			type: 'datetime'
		},
		series: [{
			data: <%= @data %>
		}],
		legend: {
			enabled: false
		}
	});
});
</script>
<% end %>

<div class="container" id="chart"></div>